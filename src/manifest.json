{
    "name": "Linkedin Profile Parser",
    "description": "Essa extenção mostra informações importantes sobre o perfil",
    "version": "0.2.0",
    "manifest_version": 2,

    "background":{
        "scripts":["./background.js"]
    },

    "browser_action":{
        "default_popup": "popup.html"
    },

    "permissions":[
        "tabs",
        "https://www.linkedin.com/"
    ]
}